<h3><%= link_to(@collection.partner_code, "/repository/" + @collection.partner_code) %></h3>

<h2><%= link_to(@collection.collection_code + ": " + @collection.title, @collection) %> # <%= @mlog_entry.media_id %></h2>

<h4><%= link_to("accession: " + @mlog_entry.accession_num, "/accession?accession=" + @mlog_entry.accession_num + "&collection=" + @collection.collection_code) %></h4>

