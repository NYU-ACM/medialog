
<br>
  <%= render 'nav' %>
  <%= render 'lookup' %>
<br/>

<h4>
  <%= link_to(MLOG_VOCAB["partner_codes"][@collection.partner_code], show_repository_path(@collection.partner_code)) %>
</h4>

<h4><%= link_to(@collection.collection_code + ": " + @collection.title, @collection) %> # <%= @mlog_entry.media_id %></h2>

<% if @accession == nil %>
  <h4>accession: </h4>
<% else %>
  <h4><%= link_to("accession: " + @accession.accession_num, @accession) %></h4>
<% end %>
<p>
  <b>created by: </b><%= @creator %><b> on: </b><%= @mlog_entry.created_at.to_s %><br/> 
  <b>modified by: </b><%= @modifier %><b> on: </b><%= @mlog_entry.updated_at.to_s %>
</p>
<br/>

